int power(int, int);\par
int main()\par
\{\par
  int n, sum = 0, t, remainder, digits = 0;\par
  printf("Input an integer\\n");\par
  scanf("%d", &n);\par
t = n;\par
  while (t != 0) \{\par
    digits++;\par
    t = t/10;\par
  \}\par
  t = n;\par
  while (t != 0) \{\par
    remainder = t%10;\par
    sum = sum + power(remainder, digits);\par
    t = t/10;\par
  \}\par
  if (n == sum)\par
    printf("%d is an Armstrong number.\\n", n);\par
  else\par
    printf("%d isn't an Armstrong number.\\n", n);\par
  return 0;\par
\}\par
int power(int n, int r) \{\par
  int c, p = 1;\par
  for (c = 1; c <= r; c++)\par
    p = p*n;\par
return p;\par
\}\par
